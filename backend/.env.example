# Backend Environment Variables
# Copy this to .env and fill in actual values

# MongoDB Connection String
# Local: mongodb://localhost:27017/autolabel
# Atlas: mongodb+srv://username:password@cluster.mongodb.net/autolabel
MONGO_URL=mongodb+srv://user:password@cluster.mongodb.net/autolabel

# OpenAI API Key
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-

# Server Port (optional, defaults to 5000)
PORT=5000
